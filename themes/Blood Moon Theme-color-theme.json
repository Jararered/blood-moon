{
    "name": "Blood Moon",
    "colors": {
        "focusBorder": "#00000000",
        "foreground": "#FFFFFF",
        "selection.background": "#FF000077",
        "list.activeSelectionBackground": "#FFFFFF22",
        "list.inactiveSelectionBackground": "#FFFFFF22",
        // "activityBar.background": "#252526",
        "statusBar.background": "#00000000",
        "button.background": "#FF0000",
        "button.hoverBackground": "#FF0000",
        "badge.background": "#FF0000",
        // Selection
        "editor.selectionForeground": "#FFFFFFFF",
        "editor.selectionBackground": "#FFFFFF44",
        "editor.inactiveSelectionBackground": "#FFFFFF44",
        "editor.selectionHighlightBackground": "#FFFFFF22",
        "editor.selectionHighlightBorder": "#00000000",
        // Match
        "editor.wordHighlightBackground": "#00000000",
        "editor.wordHighlightBorder": "#00000000",
        // Text
        "textLink.foreground": "#FF0000",
        "textLink.activeForeground": "#FF0000"
    },
    "tokenColors": [
        {
            "settings": {
                "foreground": "#FFFFFF"
            },
            "scope": []
        },
        {
            "settings": {
                "foreground": "#FF0000"
            },
            "scope": [
                "entity.other.attribute",
                "keyword.control",
                "keyword.operator.functionlike",
                "keyword.operator.wordlike",
                "keyword.other.attribute",
                "keyword.other.default",
                "keyword.other.using",
                "keyword.directive.preprocessor",
                "meta.preprocessor",
                "storage.modifier.specifier",
                "storage.modifier.virtual",
                "storage.modifier.static",
                "storage.type",
                "constant.language",
                "entity.source.cmake",
                "keyword.cmake",
                "storage.modifier.glsl",
            ]
        },
        {
            "settings": {
                "foreground": "#B14700"
            },
            "scope": "variable.other.enummember",
        },
        {
            "settings": {
                "foreground": "#FF6600"
            },
            "scope": [
                "entity.name.function.call",
                "entity.name.function.member",
                "keyword.operator.cast",
            ]
        },
        {
            "settings": {
                "foreground": "#FFDA38"
            },
            "scope": [
                "entity.name.type",
                "entity.name.function.definition",
                "storage.source.cmake",
                "variable.source.cmake",
            ]
        },
        {
            "settings": {
                "foreground": "#CFB092"
            },
            "scope": [
                "variable",
                "string",
                "entity.name.namespace",
            ]
        },
        {
            "settings": {
                "foreground": "#85743C"
            },
            "scope": [
                "comment"
            ]
        },
        {
            "settings": {
                "foreground": "#B28CC4"
            },
            "scope": [
                "constant",
                "keyword.other.unit",
            ]
        },
        {
            "settings": {
                "fontStyle": "italic"
            },
            "scope": "variable.parameter",
        }
    ]
}